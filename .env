# .env - Configuration for the Docker Deployment Application

# Application settings
PORT=3000
NODE_ENV=production

# GitHub settings
GITHUB_PAT=your_github_personal_access_token
GITHUB_OWNER=your_github_username_or_organization
GITHUB_REPO=your_repository_name

# cPanel settings
CPANEL_HOST=your_cpanel_host
CPANEL_USERNAME=your_cpanel_username
CPANEL_SSH_PORT=22
# Do not store sensitive keys in this file in production
# Set CPANEL_SSH_KEY as an environment variable or through a secrets manager

# Docker settings
DOCKER_IMAGE=your_docker_image:latest

# Security settings
SESSION_SECRET=generate_a_strong_random_string